        <property name="transportCarburant" column="transport_Carburant" type="double"/>
        <property name="transportBilletTrain" column="transport_Billet_Train" type="double"/>
        <property name="transportTaxi" column="transport_Taxi" type="double"/>
        <property name="hebergementMotel" column="hebergement_Motel" type="double"/>
        <property name="hebergementNourriture" column="hebergement_Nourriture" type="double"/>
        <property name="activiteToursOrganises" column="activite_Tours_Organises" type="double"/>
        <property name="activiteVieNocturne" column="activite_Vie_Nocturne" type="double"/>
        <property name="activiteShopping" column="activite_Shopping" type="double"/>
        <property name="activiteAutresActivites" column="activite_Autres_Activites" type="double"/>
