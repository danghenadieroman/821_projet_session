        <property name="croisiereBreuvage" column="croisiere_Breuvage" type="double"/>
        <property name="autreAssurance" column="autre_Assurance" type="double"/>
        <property name="autreDocuments" column="autre_Documents" type="double"/>
        <property name="autreImmunisation" column="autre_Immunisation" type="double"/>
        <property name="autreInternet" column="autre_Internet" type="double"/>
        <property name="autreBagages" column="autre_Bagages" type="double"/>
        <property name="autreFraisDivers" column="autre_Frais_Divers" type="double"/>
    </class>
</hibernate-mapping>
